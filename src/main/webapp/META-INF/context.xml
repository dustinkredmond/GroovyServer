<Context>
  <!-- MySQL/MariaDB connection details -->
  <Environment name="dbUser" value="mysql-username-here" type="java.lang.String"/>
  <Environment name="dbPassword" value="mysql-password-here" type="java.lang.String"/>
  <Environment name="dbUrl" value="jdbc:mariadb://localhost:3306/groovy_server" type="java.lang.String"/>

  <!-- Login information for the admin user -->
  <!-- will be automatically created, if changed via UI, UI value takes precedence -->
  <Environment name="adminUsername" value="" type="java.lang.String"/>
  <Environment name="adminPassword" value="" type="java.lang.String"/>
</Context>
